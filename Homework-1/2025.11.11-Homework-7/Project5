#include<stdio.h>

int main(int argc, char** argv)
{
double a=0;
int n=0;
    if (n == 0) {
        return 1.0;
    }
    
    if (n < 0) {
        return 1.0 / power(a, -n);
    }
    
    return a * power(a, n - 1);
}

int main() {
    double a;
    int n;
    
    std::cin >> a;
    std::cin >> n;
    
    double result = power(a, n);
    
    std::cout << std::fixed << std::setprecision(6) << result;
    
    return 0;
}
