#include<stdio.h>

int main(int argc, char** argv)
{
double a = 0;
int n = 0;
    if (n == 0) {
        return 1.0;
    }
    
    double result = 1.0;
    double base = a;
    int exponent = n;
    
    while (exponent > 0) {
        if (exponent % 2 == 1) {
            result = result * base;
        }
        base = base * base;
        exponent = exponent / 2;
    }
    
    return result;
}

int main() {
    double a;
    int n;
    
    std::cin >> a;
    std::cin >> n;
    
    double result = fastPower(a, n);
    
    std::cout << result;
    
    return 0;
}
