#include<stdio.h>

int main(int argc, char** argv)
{
int n=0;
int k=0;
    if (k == 0 || k == n) {
        return 1;
    }
    
    long long** dp = new long long*[n + 1];
    for (int i = 0; i <= n; i++) {
        dp[i] = new long long[k + 1];
    }
    
    for (int i = 0; i <= n; i++) {
        for (int j = 0; j <= std::min(i, k); j++) {
            if (j == 0 || j == i) {
                dp[i][j] = 1;
            } else {
                dp[i][j] = dp[i - 1][j - 1] + dp[i - 1][j];
            }
        }
    }
    
    long long result = dp[n][k];
    
    for (int i = 0; i <= n; i++) {
        delete[] dp[i];
    }
    delete[] dp;
    
    return result;
}

int main() {
    int n;
    int k;
    
    std::cin >> n;
    std::cin >> k;
    
    long long result = binomialCoefficient(n, k);
    
    std::cout << result;
    
    return 0;
}
