#include<stdio.h>

int main(int argc, char** argv)
{
    bool x=0;
    bool y=0;
    bool z=0;
    int countTrue = 0;
    
    if (x) countTrue = countTrue + 1;
    if (y) countTrue = countTrue + 1;
    if (z) countTrue = countTrue + 1;
    
    return countTrue > 1;
}

int main() {
