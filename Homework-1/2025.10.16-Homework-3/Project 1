#include<stdio.h>

int main(int argc, char** argv) 
{
    std::ifstream inputFile("INPUT.TXT");
    std::ofstream outputFile("OUTPUT.TXT");
    
    int N;
    inputFile >> N;
    
    if (N == 0) {
        outputFile << 0;
        return 0;
    }
    
    if (N == 1) {
        outputFile << 1;
        return 0;
    }
    
    int a = 0;
    int b = 1;
    int current;
    int counter = 1;
    
    while (counter < N) {
        current = a + b;
        a = b;
        b = current;
        counter = counter + 1;
    }
    
    outputFile << b;
    
    return 0;
}
