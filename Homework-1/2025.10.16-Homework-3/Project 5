 #include<stdio.h>

int main(int argc, char** argv)
{
    std::ifstream inputFile("INPUT.TXT");
    std::ofstream outputFile("OUTPUT.TXT");
    
    int number;
    int max1 = -1;
    int max2 = -1;
    
    inputFile >> number;
    
    do {
        if (number > max1) {
            max2 = max1;
            max1 = number;
        } else if (number > max2 && number != max1) {
            max2 = number;
        }
        
        inputFile >> number;
    } while (number != 0);
    
    outputFile << max2;
    
    return 0;
}
