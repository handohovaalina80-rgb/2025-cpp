#include<stdio.h>

int main(int argc, char** argv) 
{
    std::ifstream inputFile("INPUT.TXT");
    std::ofstream outputFile("OUTPUT.TXT");
    
    outputFile << std::fixed << std::setprecision(6);
    
    double a;
    double b;
    double c;
    
    inputFile >> a;
    inputFile >> b;
    inputFile >> c;
    
    if (a == 0) {
        if (b == 0) {
            if (c == 0) {
                outputFile << -1;
            } else {
                outputFile << 0;
            }
        } else {
            outputFile << 1 << "\n";
            double root = -c / b;
            outputFile << root;
        }
    } else {
        double discriminant = b * b - 4 * a * c;
        
        if (discriminant < 0) {
            outputFile << 0;
        } else if (discriminant == 0) {
            outputFile << 1 << "\n";
            double root = -b / (2 * a);
            outputFile << root;
        } else {
            outputFile << 2 << "\n";
            double sqrtD = sqrt(discriminant);
            double root1 = (-b - sqrtD) / (2 * a);
            double root2 = (-b + sqrtD) / (2 * a);
            
            if (root1 < root2) {
                outputFile << root1 << "\n";
                outputFile << root2;
            } else {
                outputFile << root2 << "\n";
                outputFile << root1;
            }
        }
    }
    
    return 0;
}
