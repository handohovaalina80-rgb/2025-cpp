#include<stdio.h>

int main(int argc, char** argv)
{
    std::ifstream inputFile("INPUT.TXT");
    std::ofstream outputFile("OUTPUT.TXT");
    
    int x1;
    int y1;
    int x2;
    int y2;
    
    inputFile >> x1;
    inputFile >> y1;
    inputFile >> x2;
    inputFile >> y2;
    
    int dx = abs(x2 - x1);
    int dy = abs(y2 - y1);
    
    if ((dx == 2 && dy == 1) || (dx == 1 && dy == 2)) {
        outputFile << "YES";
    } else {
        outputFile << "NO";
    }
    
    return 0;
}
