include<stdio.h>

int main(int argc, char** argv)
{
    std::ifstream inputFile("INPUT.TXT");
    std::ofstream outputFile("OUTPUT.TXT");
     
    int N;
    int A;
    int B;
    int C;
    int D;
     
    inputFile >> N;
    inputFile >> A;
    inputFile >> B;
    inputFile >> C;
    inputFile >> D;
     
    int* sequence = new int[N];
     
    for (int i = 0; i < N; i++) {
        sequence[i] = i + 1;
    }
     
    // Первый переворот от A до B
    int left = A - 1;
    int right = B - 1;
    while (left < right) {
        int temp = sequence[left];
        sequence[left] = sequence[right];
        sequence[right] = temp;
        left = left + 1;
        right = right - 1;
    }
     
    // Второй переворот от C до D
    left = C - 1;
    right = D - 1;
    while (left < right) {
        int temp = sequence[left];
        sequence[left] = sequence[right];
        sequence[right] = temp;
        left = left + 1;
        right = right - 1;
    }
     
    // Вывод результата
    for (int i = 0; i < N; i++) {
        outputFile << sequence[i];
        if (i != N - 1) {
            outputFile << " ";
        }
    }
     
    delete[] sequence;
    return 0;
}
